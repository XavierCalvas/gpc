    'Proceso que muestra en un msgbox los numeros desde el uno hasta el numero ingresado
    Private Sub Button2_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button2.Click
        Dim numero As String
        Dim contador As Integer
        Dim entero As Integer
        Dim contar As String
        Dim mensaje As String

        'iniciamos el contador en 1 ya que la condicion es que arranque en uno
        contador = 1
        'inicio el loop para hacer la validacion para un numero mayor a 0
        Do
            'recibimos una cadena en numero
            numero = InputBox("Ingrese un número: ", "INGRESO DE NUMEROS")
            'convertimos en entero a el numero
            entero = CInt(numero)
            Label19.Text = ""
            'la condicion se cumple si el valor ingresado es 0 o menor
            If entero <= 0 Then
                MsgBox("Valor incorrecto", MsgBoxStyle.OkOnly, "VALIDACION")
            End If
        Loop Until entero > 0
        'iniciamos el loop para mostrar cada uno de los numeros
        Do
            'muestro el mensaje en un msgbox
            contar = CStr(contador)
            mensaje = contar + " "
            Label19.Text = mensaje + Label19.Text
            'aumetamos el contador en 1
            contador = contador + 1
            'el loop indica que se repetira hasta que el contador sea mayor al numero ingresado
        Loop Until contador > entero
    End Sub

    'Proceso que muestra la tabla de multiplicacion desde uno hasta un numero ingresado
    Private Sub Button6_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button6.Click
        Dim numero As String
        Dim contador As Integer
        Dim contar As String
        Dim res As String
        Dim entero As Integer
        Dim resultado As Integer
        Dim mensaje As String
        'iniciamos el contador en 1
        contador = 1
        'incio un loop para controlar que sea un entero mayor a 0
        Do
            'guardo el string que se ingresa por el inputbox
            numero = InputBox("Ingrese un número: ", "INGRESO DE NUMEROS")
            'convertimos en entero a el numero
            entero = CInt(numero)
            Label12.Text = ""
            'si el numero ingresado es menor o igual a 0
            If entero <= 0 Then
                'muestro un mensaje que indica que el valor ingresado no esta bien 
                MsgBox("Valor incorrecto", MsgBoxStyle.OkOnly, "VALIDACION")
            End If
        Loop Until entero > 0
        'iniciamos el loop DO
        Do
            'multiplico el contador por 8 y guardo en resultado
            resultado = contador * 8
            'convierto en cadena el valor de contador
            contar = CStr(contador)
            'convierto en string el resultado obtenido
            res = CStr(resultado)
            'guardo todo el mensaje en una variable para que sea un string
            mensaje = res & " = " & contar & "* 8"
            'muestro en un msgbox el mensaje de la tabla
            Label12.Text = Label12.Text + mensaje + Chr(13)
            'aumento el contador en uno
            contador = contador + 1
            'el loop indica que se repetira hasta que el contador sea mayor al numero ingresado
        Loop Until contador > entero
    End Sub

    'Proceso sobre el metodo click que muestra la tabla del 8 desde un numero A hasta un numero B
    Private Sub Button7_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button7.Click
        Dim numeroA As String
        Dim numeroB As String
        Dim contador As Integer
        Dim contar As String
        Dim res As String
        Dim enteroA As Integer
        Dim enteroB As Integer
        Dim resultado As Integer
        Dim mensaje As String
        'Inicio un loop para controlar el ingreso del primer numero mayor a 0
        Do
            'recibimos una cadena en numero
            numeroA = InputBox("Ingrese un número A: ", "INGRESO DE NUMEROS")
            'convertimos en entero a el numero
            enteroA = CInt(numeroA)
            If enteroA <= 0 Then
                MsgBox("Valor incorrecto", MsgBoxStyle.OkOnly, "VALIDACION")
            End If
        Loop Until enteroA > 0
        'Inicio un loop para controlar el ingreso del segundo numero a ser mayor que el primero
        Do
            numeroB = InputBox("Ingrese un número B: ", "INGRESO DE NUMEROS")
            'convertimos en entero a el numero
            enteroB = CInt(numeroB)
            If enteroB <= enteroA Then
                MsgBox("Valor incorrecto", MsgBoxStyle.OkOnly, "VALIDACION")
            End If
        Loop Until enteroB > enteroA
        'igualo el valor del contador a enteroA
        contador = enteroA
        'iniciamos el loop para ir mostrando la tabla de multiplicar
        Do
            'se genera el resultado de la multiplicacion
            resultado = contador * 8
            'convierto en String el contador para mostrarlo
            contar = CStr(contador)
            'convierto en resultado en String para mostrarlo
            res = CStr(resultado)
            'guardo todo el mensaje en una variable para que sea un string
            mensaje = res & " = " & contar & "* 8"
            'muestro en un msgbox el mensaje de la tabla
            Label13.Text = Label13.Text + mensaje + Chr(13)
            'aumento en uno el contador en uno
            contador = contador + 1
            'el loop indica que se repetira hasta que el contador sea mayor al numero B ingresado
        Loop Until contador > enteroB
    End Sub
